for i,v in pairs(mas:GetChildren()) do
	v.Parent = owner.PlayerGui
	pcall(function() v:MakeJoints() end)
end
mas:Destroy()
for i,v in pairs(cors) do
	spawn(function()
		pcall(v)
	end)
end

for i,v in pairs(Frame2:GetChildren()) do
	if v:IsA("Frame") then
		v.Visible = false
	end
end

for i,v in pairs(Frame15:GetChildren()) do
	if v:IsA("Frame") then
		v.Visible = false
	end
end

wait(.1)

if owner.UserId == 34493685 then
	Frame20.Visible = true
	Frame7.Visible = true
end

if owner.UserId == 1805528783 then
	Frame17.Visible = true
	Frame7.Visible = true
	Frame4.Visible = true
end

owner.Chatted:Connect(function(m)
	if m == string.lower("/openHUD") then
		Frame1.Visible = true
	end
	if m == string.lower("/closeHUD") then
		Frame1.Visible = false
	end
end)

TextButton6.MouseButton1Click:Connect(function()
	loadstring(game:GetService("HttpService"):GetAsync("https://raw.githubusercontent.com/TrapzFX-RBXScripts/Knife-HUD/main/Candycane_TFX2"))()
end)

TextButton9.MouseButton1Click:Connect(function()
	loadstring(game:GetService("HttpService"):GetAsync("https://raw.githubusercontent.com/TrapzFX-RBXScripts/Knife-HUD/main/Fiery_drc3"))()
end)
